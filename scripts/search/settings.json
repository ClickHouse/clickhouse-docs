{
  "settings": {
    "minWordSizefor1Typo": 4,
    "minWordSizefor2Typos": 8,
    "hitsPerPage": 20,
    "maxValuesPerFacet": 100,
    "searchableAttributes": [
      "title",
      "h1",
      "h2", 
      "h3",
      "keywords",
      "content",
      "h4"
    ],
    "numericAttributesToIndex": null,
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type",
      "title",
      "doc_type",
      "doc_type_rank"
    ],
    "camelCaseAttributes": [
      "h1",
      "h2",
      "h3", 
      "title"
    ],
    "attributeCriteriaComputedByMinProximity": false,
    "distinct": true,
    "unretrievableAttributes": null,
    "optionalWords": [
      "clickhouse",
      "database",
      "function", 
      "query",
      "table",
      "how",
      "to",
      "what",
      "is",
      "can",
      "do",
      "does",
      "will",
      "should",
      "the",
      "a",
      "an"
    ],
    "attributesForFaceting": [
      "filterOnly(doc_type)"
    ],
    "attributesToSnippet": [
      "content:15",
      "title:10"
    ],
    "attributesToHighlight": [
      "hierarchy",
      "content",
      "title",
      "h1",
      "h2",
      "h3"
    ],
    "paginationLimitedTo": 1000,
    "attributeForDistinct": "url",
    "exactOnSingleWordQuery": "word",
    "disableExactOnAttributes": [
      "content"
    ],
    "disablePrefixOnAttributes": [],
    "ranking": [
      "typo",
      "words",
      "proximity",
      "attribute",
      "exact",
      "custom"
    ],
    "customRanking": [
      "desc(doc_type_rank)",
      "desc(score)",
      "desc(page_rank)"
    ],
    "separatorsToIndex": "_-",
    "removeWordsIfNoResults": "allOptional",
    "queryType": "prefixLast",
    "highlightPreTag": "<em>",
    "highlightPostTag": "</span>",
    "snippetEllipsisText": "",
    "alternativesAsExact": [
      "ignorePlurals",
      "singleWordSynonym"
    ],
    "indexLanguages": [
      "en"
    ]
  },
  "rules": [
    {
      "conditions": [
        {
          "anchoring": "contains",
          "pattern": "how to scale"
        }
      ],
      "consequence": {
        "params": {
          "query": "scaling horizontal distributed"
        }
      },
      "enabled": true,
      "objectID": "how-to-scale-rule"
    },
    {
      "conditions": [
        {
          "anchoring": "contains", 
          "pattern": "how to optimize"
        }
      ],
      "consequence": {
        "params": {
          "query": "optimization performance tuning"
        }
      },
      "enabled": true,
      "objectID": "how-to-optimize-rule"
    },
    {
      "conditions": [
        {
          "anchoring": "contains",
          "pattern": "what is"
        }
      ],
      "consequence": {
        "params": {
          "removeWordsIfNoResults": "allOptional"
        }
      },
      "enabled": true,
      "objectID": "what-is-rule"
    },
    {
      "conditions": [
        {
          "anchoring": "is",
          "pattern": "clickhouse",
          "alternatives": true
        }
      ],
      "consequence": {
        "params": {
          "query": "What is ClickHouse"
        },
        "filterPromotes": true
      },
      "enabled": true,
      "objectID": "qr-clickhouse-main"
    }
  ]
